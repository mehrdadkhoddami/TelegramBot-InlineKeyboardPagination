    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^6.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "TelegramBot\\InlineKeyboardPagination\\": "src"
        }
